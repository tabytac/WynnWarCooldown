{
	"schemaVersion": 1,
	"id": "wynn-war-cooldown",
	"version": "${version}",
	"name": "Wynn War Cooldown",
	"description": "A simple mod that tracks the cooldown of Guild Wars in Wynncraft and displays it on the screen.",
	"authors": [
		"Tabytac"
	],
	"contact": {
		"sources": "https://github.com/Tabytac/WynnWarCooldown"
	},
	"license": "CC-BY-NC 4.0",
	"icon": "assets/wynn-war-cooldown/icon.png",
	"environment": "client",
	"entrypoints": {
		"main": [
			{
				"value": "wynnwarcooldown.tabytac.WynnWarCooldown",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "wynnwarcooldown.tabytac.WynnWarCooldownClient",
				"adapter": "kotlin"
			}
		],
		"modmenu": [
			{
				"value": "wynnwarcooldown.tabytac.ModMenuIntegration",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"wynn-war-cooldown.mixins.json",
		"wynn-war-cooldown.client.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.10",
		"minecraft": "~1.21.4",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*",
		"wynntils": ">=3.0.0"
	},
	"suggests": {
		"modmenu": "*"
	}
}